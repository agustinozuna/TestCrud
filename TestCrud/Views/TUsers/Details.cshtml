@model TestCrud.Models.TUsers

@{
    ViewData["Title"] = "Detalle";
}

<h1>Detalle</h1>

<div>
    <h4>TUsers</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Usuario
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TxtUser)
        </dd>
        <dt class = "col-sm-2">
            Nombre
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TxtNombre)
        </dd>
        <dt class = "col-sm-2">
           Apellido
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TxtApellido)
        </dd>
        <dt class = "col-sm-2">
           Nro. Doc.
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NroDoc)
        </dd>
        <dt class = "col-sm-2">
            Activo
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.SnActivo)
        </dd>
        <dt class = "col-sm-2">
            Rol
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CodRolNavigation.TxtDesc)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.CodUsuario">Editar</a> |
    <a asp-action="Index">Regresar</a>
</div>
